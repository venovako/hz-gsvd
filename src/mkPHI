	$(MAKE) -C DGSVD -f mkPHI clean
	$(MAKE) -C exe0 -f mkPHI clean
	$(MAKE) -C exe1 -f mkPHI clean
	$(MAKE) -C exe2 -f mkPHI clean
#	$(MAKE) -C exe3 -f mkPHI clean
